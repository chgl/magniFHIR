{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http.HttpClient": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FhirServers": [],
  "ResourceBrowsers": {
    "Patient": {
      "SortBy": "_lastUpdated"
    },
    "Condition": {
      "SortBy": "recorded-date",
      "PageSize": 25,
      "Columns": [
        {
          "Header": "id",
          "Path": "id"
        },
        {
          "Header": "recorded",
          "Path": "recordedDate"
        },
        {
          "Header": "code",
          "Path": "code"
        },
        {
          "Header": "clinical status",
          "Path": "clinicalStatus.coding.code"
        },
        {
          "Header": "verification status",
          "Path": "verificationStatus.coding.code"
        }
      ]
    },
    "Observation": {
      "SortBy": "_lastUpdated",
      "Columns": [
        {
          "Header": "id",
          "Path": "id"
        },
        {
          "Header": "effective",
          "Path": "effective"
        },
        {
          "Header": "code",
          "Path": "code"
        },
        {
          "Header": "value",
          "Path": "value"
        }
      ]
    },
    "Encounter": {
      "SortBy": "date",
      "Columns": [
        {
          "Header": "id",
          "Path": "id"
        },
        {
          "Header": "location",
          "Path": "location.location.display"
        }
      ]
    },
    "Procedure": {
      "SortBy": "date",
      "Columns": [
        {
          "Header": "id",
          "Path": "id"
        },
        {
          "Header": "performed",
          "Path": "performed.start"
        },
        {
          "Header": "code",
          "Path": "code"
        },
        {
          "Header": "status",
          "Path": "status"
        }
      ]
    }
  },
  "Tracing": {
    "Enabled": false,
    "ServiceName": "magniFHIR",
    "Exporter": "jaeger",
    "Jaeger": {},
    "Otlp": {
      "Endpoint": ""
    }
  }
}
