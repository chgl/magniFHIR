{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http.HttpClient": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FhirServers": [],
  "ResourceBrowsers": {
    "Patient": {
      "SortBy": "_lastUpdated"
    },
    "Condition": {
      "SortBy": "recorded-date",
      "PageSize": 25,
      "Columns": [
        {
          "Header": "id",
          "Path": "Condition.id"
        },
        {
          "Header": "recorded",
          "Path": "Condition.recordedDate"
        },
        {
          "Header": "text, system, code",
          "Path": "Condition.code.text | Condition.code.coding.system | Condition.code.coding.code"
        },
        {
          "Header": "clinical status",
          "Path": "Condition.clinicalStatus.coding.code"
        },
        {
          "Header": "verification status",
          "Path": "Condition.verificationStatus.coding.code"
        }
      ]
    },
    "Observation": {
      "SortBy": "_lastUpdated",
      "Columns": [
        {
          "Header": "id",
          "Path": "Observation.id"
        },
        {
          "Header": "effective",
          "Path": "Observation.effective"
        },
        {
          "Header": "text, system, code",
          "Path": "Observation.code.text | Observation.code.coding.system | Observation.code.coding.code"
        }
      ]
    },
    "Encounter": {
      "SortBy": "date",
      "Columns": [
        {
          "Header": "id",
          "Path": "Encounter.id"
        },
        {
          "Header": "location",
          "Path": "Encounter.location.location.display"
        }
      ]
    },
    "Procedure": {
      "SortBy": "date",
      "Columns": [
        {
          "Header": "id",
          "Path": "Procedure.id"
        },
        {
          "Header": "performed",
          "Path": "Procedure.performed.start"
        },
        {
          "Header": "text, system, code",
          "Path": "Procedure.code.text | Procedure.code.coding.system | Procedure.code.coding.code"
        },
        {
          "Header": "status",
          "Path": "Procedure.status"
        }
      ]
    }
  },
  "Tracing": {
    "Enabled": false,
    "ServiceName": "magniFHIR",
    "Exporter": "jaeger",
    "Jaeger": {},
    "Otlp": {
      "Endpoint": ""
    }
  }
}
